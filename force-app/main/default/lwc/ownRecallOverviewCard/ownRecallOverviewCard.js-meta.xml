<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>52.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property label="Icon" name="icon" type="String" />
            <property label="Title" name="title" type="String" />
            <property label="Title Color" name="titlecolor" type="String" datasource="Black,Honda Red"/>
            <property label="Action Icon" name="actionicon" type="String" />
            <property label="Action Title" name="actiontitle" type="String" />
            <property label="Show Footer" name="showFooter" type="Boolean" />
            <property label="Brand" name="brand" type="String" />
            <property label="Brand Name" name="brandName" type="String" datasource="Honda,Acura,Powersports,Powerequipment,Marine" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>